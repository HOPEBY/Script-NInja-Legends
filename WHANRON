do
	local ui = game:GetService("CoreGui"):FindFirstChild("FluxLib")
	if ui then
		ui:Destroy()
	end
end

local Flux = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/fluxlib.txt")()

local win = Flux:Window("NINJALEGENDS", "BY WHANRON ðŸŒ™", Color3.fromRGB(241, 15, 135), Enum.KeyCode.P)
local tab = win:Tab("Auto Farm ðŸ’²", "http://www.roblox.com/asset/?id=6023426915")

tab:Label("Auto Farm ðŸ’²", "", function()
end)
tab:Toggle("Auto Swing", "Auto Swing", false, function(t)
    _G.A = t
    if _G.A == false then return end
    while _G.A do wait()

local args = {
    [1] = "swingKatana"
}
game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(unpack(args))

end
end)
tab:Toggle("Auto Sell", "Auto Sell", false, function(t)
    _G.A = t 
if _G.A == false then return end
while _G.A do wait()

        if game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart") then
        game.workspace.sellAreaCircles["sellAreaCircle7"].circleInner.CFrame = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame
        wait(.1)
        game.workspace.sellAreaCircles["sellAreaCircle7"].circleInner.CFrame = game.Workspace.Part.CFrame
        end
        end
    end)

tab:Line()

local tab2 = win:Tab("Movement", "http://www.roblox.com/asset/?id=6022668888")

tab2:Label("MOVEMENT YOUR CAHRACTER")
tab2:Slider("Walkspeed", "Makes your faster then The Flash bitch!!!", 0, 1000,0,function(t)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = t
end)
tab2:Slider("JumpPower", "Can make your Jump to the fucking hell bitch!", 0, 1000,0,function(t)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = t
end)
tab2:Line()
